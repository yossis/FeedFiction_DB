<h1>Please choose image and start your story</h1>
<div id="story-container" role="story-container">
	<ul id="tiles" class="unstyled">
		<% @images.each do |image|%>
			<li class="story-box container-ss">
				<% ratio = aspect_ratio(image['src_big_width'], image['src_big_height'], 365) %>
				<%=  image_tag(image['src_big'] , width: ratio['new_width'].to_s+'px' , height: ratio['new_height'].to_s+'px')  %>
				<div class="tiels-items-container">
					<a class="btn btn-small"><i class="icon-edit"></i>Start Story</a>		
				</div>

			</li>

		<% end %>
	</ul>
</div>
<div id="modal-upload">

</div>
<div class="modal hide" id="start-story">
  <div class="modal-header lg">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
    <h2>Start A Story</h2>
  </div>
   <%= render 'start_story' %>
</div>
	
</div>

