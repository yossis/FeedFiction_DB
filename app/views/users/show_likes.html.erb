<% provide(:title, @title) %>
<section id="user-header">
  <%= render 'user_header' %>
</section>
<section>
	<% if @stories.count>0 %>
	<h2><%= @user.name %> likes the stories:</h2>
	<%= render 'feeds/feed' %>
	<%else%>
	<div class="alert">
		<% if @user==current_user %>
			<h3>You do not have like yet!</h3> 
		<% else %>
			<h3><%= @user.name %> dosen't has like yet</h3>
		<% end%>
	</div>
	<%end%>
</section>