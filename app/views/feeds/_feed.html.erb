<% if (current_user && current_user.stories.size<1) %>
	<div class="alert alert-block">
	  <button class="btn btn-primary userFunction" data-function="startStoryModal" type="button" >Start here</button>
	</div>

<%end%>

<div id="story-container" >
	<div id="portfolio-wrapper" class="row">
	<%= render :partial => 'stories/story' ,:collection => @stories%>
	</div>
</div>
<%= will_paginate @stories%>