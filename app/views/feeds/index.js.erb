var $newItems = $("<%= escape_javascript(render(:partial => 'stories/story' , :collection => @stories))%>")
<% if @stories.next_page %>
$('.pagination').replaceWith('<%= j will_paginate(@stories) %>');
<% else %>
$('.pagination').remove();
<% end %>

$('#portfolio-wrapper').append($newItems).isotope('appended', $newItems)
